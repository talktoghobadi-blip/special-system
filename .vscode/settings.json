{
    "workbench.editor.limit.enabled": true,
    "workbench.editor.defaultBinaryEditor": "vscode.markdown.preview.editor",
    "workbench.editor.autoLockGroups": {
        "default": true,
        "workbench.editorinputs.searchEditorInput": true,
        "notebookOutputEditor": true,
        "jupyter-notebook": true
    },
    "workbench.editor.limit.perEditorGroup": true,
    "terminal.integrated.allowChords": false,
    "files.encoding": "utf8bom",
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "GitHub.copilot-chat",
    "editor.wordWrap": "on",
    "editor.allowVariableLineHeights": false,
    "workbench.commandPalette.experimental.suggestCommands": true,
    "workbench.experimental.cloudChanges.autoStore": "onShutdown",
    "workbench.experimental.cloudChanges.partialMatches.enabled": true,
    "workbench.experimental.share.enabled": true,
    "workbench.list.defaultFindMatchType": "contiguous",
    "workbench.panel.defaultLocation": "right",
    "workbench.quickOpen.preserveInput": true,
    "workbench.secondarySideBar.defaultVisibility": "visible",
    "workbench.startupEditor": "welcomePage",
    "workbench.activityBar.location": "top",
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.preferredDarkColorTheme": "GitHub Dark Dimmed",
    "workbench.preferredHighContrastColorTheme": "GitHub Light Colorblind (Beta)",
    "workbench.fontAliasing": "auto",
    "workbench.iconTheme": "vs-minimal",
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "workbench.tree.enableStickyScroll": false,
    "workbench.view.alwaysShowHeaderActions": true,
    "workbench.view.showQuietly": {
        "workbench.panel.output": true
    },
    "breadcrumbs.symbolPath": "last",
    "breadcrumbs.symbolSortOrder": "name",
    "workbench.editor.alwaysShowEditorActions": true,
    "workbench.editor.editorActionsLocation": "titleBar",
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.sharedViewState": true,
    "workbench.editor.titleScrollbarSizing": "large",
    "workbench.editor.titleScrollbarVisibility": "visible",
    "workbench.editor.untitled.labelFormat": "name",
    "workbench.editor.wrapTabs": true,
    "workbench.settings.openDefaultKeybindings": true,
    "workbench.settings.openDefaultSettings": true
}